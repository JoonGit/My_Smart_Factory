@model IEnumerable<My_Smart_Factory.Data.Vo.Insp.InspSpecVo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.InspSpecName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProdName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InspEquipName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IES)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ETR)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InspSpecName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProdName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InspEquipName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IES)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ETR)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
